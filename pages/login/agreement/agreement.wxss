.container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  color: black;
}
.container_header{
  width: 100%;
  line-height: 80rpx;
  display: flex;
  justify-content: center;
  font-size: 12pt;
  font-weight: bold;
  margin-top: 30rpx;
}
.container_body{
  width: 95%;
  padding:0 30rpx;
  font-size: 11pt;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.container_body_header{
  text-indent:25px;
  width: 100%;
}
.content{
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
}
.content_item{
  font-weight: bold;
  font-size: 11pt;
}
.content_item_text{
  font-size: 10pt;
  margin-top: 20rpx;
}