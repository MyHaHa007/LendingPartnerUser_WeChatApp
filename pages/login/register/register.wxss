.container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.register{
  width: 100%;
  line-height: 80rpx;
  padding: 20rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.register_telphone,.register_weixin{
  width: 50%;
  line-height: 60rpx;
  display: flex;
  justify-content: center;
     border-bottom: 4rpx solid #eee;
}
#telphone{
  border-right: 4rpx solid #eee;
}
.register_information{
  padding: 20rpx;
  border: 2rpx solid #eee;
  border-radius: 10rpx;
  margin : 20rpx 30rpx;
}
.register_agreement .agreement{
  border-bottom: 2rpx solid darkgrey;
}
.register_agreement{
   padding: 30rpx;
   font-size: 11pt;
   color: darkgrey;
   align-items: center;
}
.register_button{
    margin : 30rpx 30rpx;
}
.content{
  margin-top: 50rpx;
  display: flex;
  justify-content: center;
  color: red;
  font-weight: bold;
}
.register_place{
  padding: 20rpx;
  margin : 30rpx 30rpx;
  border-bottom: 2rpx solid #eee;
}