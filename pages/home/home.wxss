Page{
    background: whitesmoke;
}
.container{
    width: 100%;
    display: flex;
    flex-direction: column;
}
.container_head{
    width: 100%;
    height: 200rpx;
    background: #39904F;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.images image{
    width: 120rpx;
    height: 120rpx;
    border-radius: 100%;
    margin-left: 30rpx;
    margin-right: 20rpx;
}
.dataname,.dataexplain{
    /*margin-left: 10rpx;*/
    padding-bottom: 10rpx;
}
.content{
    width: 95%;
    line-height: 100rpx;
    display: flex;
    flex-direction: row;
    margin-left: 10rpx;
    margin: 0 20rpx;
    align-items: center;
    border-bottom: 2rpx solid #eee;
}

.content .item image{
    width: 50rpx;
    height: 50rpx;
    margin-top: 30rpx;
}
.img{
    padding-bottom: 50rpx;
}
.content .img image{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    right: 10rpx;
}
.content .text{
    margin-left: 20rpx;
    font-size: 11pt;
}
.data_icon{
    display: flex;
    font-size: 11pt;
    flex-direction: column;

}
.data_icon image{
    width: 45rpx;
    height: 20px;
    margin-left: 20rpx;
}
.login{
  border: 2rpx solid #eee;
  color: #eee;
  padding: 5rpx;
  font-size: 10pt;
  font-weight: bold;
  font-family:YouYuan;
}